# Example environment variables for new_project_template
# Copy this to .env and add your actual API keys

# AI API KEYS
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here
XAI_API_KEY=your_xai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here
GITHUB_TOKEN=your_github_token_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Used by Bedrock
AWS_PROFILE=default
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Search APIs (optional)
GOOGLE_CSE_ID=your_google_cse_id_here
GOOGLE_CSE_API_KEY=your_google_cse_api_key_here
SERPER_API_KEY=your_serper_api_key_here
SERPER_API_KEY_GOOGLE=your_serper_google_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here
JINA_API_KEY=your_jina_api_key_here
BRAVE_API_KEY=your_brave_api_key_here
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# Misc APIs (optional)
WEATHERAPI_KEY=your_weather_api_key_here
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_pat_here

# Tracing (optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=par_ai

# Application Options (override config file)
PARGPT_AI_PROVIDER=OpenAI
PARGPT_MODEL=
PARGPT_PRICING=price
PARGPT_DISPLAY_OUTPUT=md
PARGPT_DEBUG=false
PARGPT_SHOW_CONFIG=false
PARGPT_AGENT_MODE=false
PARGPT_NO_REPL=true
PARGPT_MAX_ITERATIONS=5
PARGPT_YES_TO_ALL=false
PARGPT_SHOW_TOOL_CALLS=true